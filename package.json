    "@types/react-dom": "^16.0.7",
    "parcel-bundler": "^1.9.7",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.26",
    "tsconfig-paths": "^3.5.0",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
